<Window x:Class="ChartView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChartView"
        mc:Ignorable="d"
        Title="MainWindow" Height="665" Width="391">
    <Grid Height="406" Margin="0,0,0
          ,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="111*"/>
            <ColumnDefinition Width="160*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="letsGoButton" Content="Start Publishing..." HorizontalAlignment="Left" Margin="55,-86,0,0" VerticalAlignment="Top" Width="146" Click="LetsGoButton_Click" Height="18" Grid.Column="1"/>
        <TextBox x:Name="keywordBox" HorizontalAlignment="Left" Margin="10,-86,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="126"/>
        <TextBox x:Name="consoleBox" Text="TextBox" TextWrapping="Wrap" HorizontalAlignment="Left" Width="351" Grid.ColumnSpan="2" Margin="10,-44,0,-90" />

    </Grid>
</Window>
